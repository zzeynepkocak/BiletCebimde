                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
   COALESCE(NakitTablosu.MusteriID, VarlikTablosu.MusteriID) AS MusteriID,
   ISNULL(NakitTablosu.NakitTRY, 0) AS NakitTRY,
   ISNULL(VarlikTablosu.VarlikTRY, 0) AS VarlikTRY,
   (ISNULL(NakitTablosu.NakitTRY, 0) + ISNULL(VarlikTablosu.VarlikTRY, 0)) AS ServetTRY
FROM
   (SELECT H.MusteriID, SUM(H.Meblag * DT.Kur) AS NakitTRY
    FROM dbo.HESAP H
