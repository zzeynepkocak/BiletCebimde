                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
    H.MusteriID,
    H.AcilmaTarihi,
    S.SehirAd,
    B.BolgeAd,
    ROW_NUMBER() OVER (PARTITION BY H.MusteriID ORDER BY H.AcilmaTarihi ASC) AS SiraNo
FROM dbo.HESAP H
INNER JOIN dbo.BANKA_SUBE BS ON H.SubeID = BS.SubeID
INNER JOIN dbo.ILCE I ON BS.IlceID = I.IlceID
INNER JOIN dbo.SEHIR S ON I.SehirID = S.SehirID
INNER JOIN dbo.BOLGE B ON S.BolgeID = B.BolgeID</property>
