                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=17.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">/* SADECE İLK 3 KAYDI GETİREN SORGULU VERSİYON */
SELECT * FROM
(
    SELECT 
        M.MusteriID,
        M.KategoriID,
        CASE 
            WHEN M.IkinciAd IS NOT NULL AND M.IkinciAd &lt;&gt; '' THEN M.IlkAd + ' ' + M.IkinciAd + ' ' + M.Soyad
            ELSE M.IlkAd + ' ' + M.Soyad
        END AS TamAd,
        K.SehirAd,
        K.BolgeAd,
        DATEDIFF(YEAR, M.DTarihi, '2025-11-16') AS Yas,
        S.ServetTRY,
        /* Sıralama: Bölge ve Kategori içinde, En Genç ve En Zengin */
        DENSE_RANK() OVER (
            PARTITION BY K.BolgeAd, M.KategoriID 
            ORDER BY DATEDIFF(YEAR, M.DTarihi, '2025-11-16') ASC, S.ServetTRY DESC
        ) AS SiraNo
    FROM dbo.MUSTERI M
    INNER JOIN dbo.GeciciMusteriKonum K ON M.MusteriID = K.MusteriID
    INNER JOIN dbo.GeciciMusteriServeti S ON M.MusteriID = S.MusteriID
) AS T
WHERE T.SiraNo &lt;= 3  /* İŞTE SİHİRLİ DOKUNUŞ BURADA */</property>
